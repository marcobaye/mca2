	cr	= 13
	; new color codes (because some PetSCII codes clash with the UTF8 conversion)
	color_BLACK	= $f0
	color_WHITE	= $f1
	color_RED	= $f2
	color_CYAN	= $f3
	color_PURPLE	= $f4
	color_GREEN	= $f5
	color_BLUE	= $f6
	color_YELLOW	= $f7
	color_ORANGE	= $f8
	color_BROWN	= $f9
	color_LRED	= $fa
	color_GRAY1	= $fb
	color_GRAY2	= $fc
	color_LGREEN	= $fd
	color_LBLUE	= $fe
	color_GRAY3	= $ff

	+autogenerated	; now include contents of "_autogenerated.a"

; external storage:
softstack_lo
	* = * + MAX_RECURSIONS
softstack_hi
	* = * + MAX_RECURSIONS
directions_lo
	* = * + 6	; n/s/w/e/u/d
directions_hi
	* = * + 6	; n/s/w/e/u/d
gamevars_lo
	* = * + gamevars_COUNT
gamevars_hi
	* = * + gamevars_COUNT

	!if * >= $a000 { !error "Reached BASIC ROM. Please add code to disable it." }
